{
    "manifest_version": 3,
    "name": "Scrapbook Helper",
    "version": "1.0",
    "description": "SFGame scrapbook helper",
    "permissions": [
        "webRequest",
        "declarativeNetRequest",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.sfgame.cz/",
                "*://*.sfgame.net/",
                "*://*.sfgame.eu/",
                "*://*.sfgame.de/",
                "*://*.sfgame.pl/",
                "*://*.sfgame.hu/",
                "*://*.sfgame.ae/",
                "*://*.sfgame.br/",
                "*://*.sfgame.ca/",
                "*://*.sfgame.cl/",
                "*://*.sfgame.dk/",
                "*://*.sfgame.es/",
                "*://*.sfgame.fr/",
                "*://*.sfgame.co.uk/",
                "*://*.sfgame.gr/",
                "*://*.sfgame.in/",
                "*://*.sfgame.it/",
                "*://*.sfgame.jp/",
                "*://*.sfgame.mx/",
                "*://*.sfgame.nl/",
                "*://*.sfgame.pt/",
                "*://*.sfgame.ro/",
                "*://*.sfgame.ru/",
                "*://*.sfgame.se/",
                "*://*.sfgame.sk/",
                "*://*.sfgame.tr/",
                "*://*.sfgame.us/"
            ],
            "js": [ "pageload.js" ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [{
        "resources": [
            "script.js",
            "playeritems.js",
            "scrapbook.js"
        ],
        "matches": [ "<all_urls>" ]
     }]
}